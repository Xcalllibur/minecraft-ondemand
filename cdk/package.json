{
    "name": "cdk",
    "version": "0.1.0",
    "bin": {
        "cdk": "bin/minecraft-ondemand.js"
    },
    "scripts": {
        "build": "tsc",
        "watch": "tsc -w",
        "cdk": "cdk",
        "synth": "cdk synth --profile minecraft",
        "deploy": "cdk deploy --all --require-approval never --profile minecraft",
        "destroy": "cdk destroy --all --profile minecraft"
    },
    "devDependencies": {
        "@aws-cdk/assert": "1.124.0",
        "@types/node": "16.9.6",
        "aws-cdk": "1.124.0",
        "ts-node": "^10.2.1",
        "typescript": "~4.4.3"
    },
    "dependencies": {
        "@aws-cdk/aws-ec2": "^1.124.0",
        "@aws-cdk/aws-ecs": "^1.124.0",
        "@aws-cdk/aws-efs": "^1.124.0",
        "@aws-cdk/aws-iam": "^1.124.0",
        "@aws-cdk/aws-lambda-python": "^1.124.0",
        "@aws-cdk/aws-logs": "^1.124.0",
        "@aws-cdk/aws-logs-destinations": "^1.124.0",
        "@aws-cdk/aws-sns": "^1.124.0",
        "@aws-cdk/core": "1.124.0",
        "source-map-support": "^0.5.16"
    }
}
